<link href="css/estilos.css" rel="sylesheet">
    <% include partial/header.ejs %>

        <div class="container" style=" margin-top: 3%;">
            <div class="row">
                <div class="col-md-5">
                    <img src="imagenes/sombrero.PNG" class="img-fluid mt-5">
                </div>
                <div class="col-md-7 form-group mt-5">
                    <h3 style="color: #d8a10a" style="margin-top:3%;">INSUMOS</h3>
                    <h5 style="color: #93949B"><em>Nuevo producto</em></h5>
                    <form id="registerInventory" action="#" method="POST" class="mt-5">
                        <div class="form-group row">
                            <div class="col-sm-10">
                                <select class="form-control" id="categoria" type="select" name="categoria">
                                    <option  value="Null">Seleccione una categoria del insumo</option>
                                    <option  value="Alfarería">Alfarería</option>
                                    <option  value="Bisuteria">Bisuteria</option>
                                    <option  value="Ceramica">Ceramica</option>
                                    <option  value="Cuero">Cuero</option>
                                    <option  value="Manualidades">Manualidades</option>
                                    <option  value="Tallado">Tallado</option>
                                    <option  value="Textiles">Textiles</option>
                                    <option  value="Tornero">Tornero</option>
                                    <option  value="Otro">Otro</option>
                                </select>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-sm-2 col-form-label">Subcategoria</label>
                            <div class="col-sm-10">
                                <input class="form-control" id="subCategoria" name="subCategoria" type="text" placeholder="Sub-Categoria del insumo" required>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-sm-2 col-form-label">Nombre</label>
                            <div class="col-sm-10">
                                <input class="form-control" id="nombre" name="nombre" type="text" placeholder="Nombre del insumo" required>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-sm-2 col-form-label">Precio</label>
                            <div class="col-sm-10">
                                <input class="form-control" type="text" name="precio" id="precio" placeholder="Precio del insumo" required>
                            </div>
                        </div>
                        <div class="form-group row">
                            <label class="col-sm-2 col-form-label">Cantidad</label> 
                            <div class="col-sm-10">
                                <input class="form-control" type="number" name="cantidad" id="cantidad" placeholder="Cantidad del insumo" required>
                            </div>
                        </div>
                        <div class="form-group row">
                            <div class="col-sm-12 text-center">
                                <input type="submit" class="btn-get-started2" style="background-color:white;">
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>

    <!--================Scripts============================= -->
    <script src="js/inventory.js"></script>
    <script src="js/jquery.sticky.js"></script>
    <script src="js/parallax.min.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script src="js/jquery.magnific-popup.min.js"></script>
    <script src="js/perfilx.js"></script>
    <script type="text/javascript" src="JS/jquery-3.3.1.min.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
 </body>
</html>
