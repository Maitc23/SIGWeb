<html lang="en">
<head>
    <title>Inventario</title>

    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
</head>
<body>
    <h1>Inventario aqui</h1>
    <h1>Insumos</h1>
    <div class="container">
        <div class="row">
            <div class="col-md-10 form-group">
                <form action="" method="POST">
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label">Categoria:</label>
                        <div class="col-sm-10">
                            <select class="form-control" id="categoria">
                                <option value="null">------------------</option>
                                <option value="">opcion 1</option>
                                <option value="">opcion 2</option>
                                <option value="">opcion 3</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label">Nombre:</label>
                        <div class="col-sm-10">
                            <input class="form-control" id="nombre" name="" type="text" placeholder="nombre del insumo"required>
                        </div>
                    </div>
                    <div class="form-group row">
                            <label class="col-sm-2 col-form-label">Sub-Categoria:</label>
                            <div class="col-sm-10">
                                <input class="form-control" id="subCategoria" name="" type="text" placeholder="Sub-Categoria">
                            </div>
                        </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label">Precio:</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" name="" id="precio" placeholder="precio 1.50$" required>
                        </div>
                    </div>
                    <div class="form-group row">
                        <label class="col-sm-2 col-form-label">Cantidad:</label>
                        <div class="col-sm-10">
                            <input class="form-control" type="text" name="" id="cantidad" placeholder="cantidad 15 und." required>   
                        </div>
                    </div>
                    <div class="form-group row">
                        <div class="col-sm-12">
                        <button type="submit" class="btn btn btn btn-outline-success btn-lg btn-block ">Guardar</button>
                        </div>
                    </div>
                 </form>
            </div>
        </div>   
    </div>

    <!--================Scripts============================= -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="js/jquery.min.js"></script>
    <script src="js/jquery.sticky.js"></script>
    <script src="js/parallax.min.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script src="js/jquery.magnific-popup.min.js"></script>
    <script src="js/perfilx.js"></script>
</body>
</html>